.fulimall{
  width: 100%;
  background-color: #f5f5f5;
  padding: 0 12rpx;
  box-sizing: border-box;
}

.fulimall-title-img{
  width: 100%;
  height: 250rpx;
}

.fulimall-title-img image{
  width: 100%;
  height: 100%;
}

.fulimall-goods-wrapper{
  width: 100%;
  margin-top: 12rpx;
  display: flex;
  flex-wrap: wrap;
}

.fulimall-goods-item{
  width: 234rpx;
  height: 352rpx;
  background-color: #fff;
  margin: 0 12rpx 12rpx 0;
  border-radius: 10rpx;
}

.fulimall-goods-item:nth-child(3n+3){
  margin-right: 0;
}

.fulimall-goods-item-img{
  width: 100%;
  height: 232rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.fulimall-goods-item-img image{
  width: 100%;
  height: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.fulimall-goods-item-content{
  width: 100%;
  padding: 0 12rpx;
  box-sizing: border-box;
}

.fulimall-goods-item-name{
  color: #333;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.fulimall-goods-item-shop-price{
  color: #999;
  font-size: 20rpx;
  margin: 5rpx 0;
}

.fulimall-goods-item-price{
  color: #fe6601;
  font-size: 30rpx;
}

.fulimall-special{
  width: 100%;
}

.fulimall-special-title{
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  color: #333;
  font-size: 28rpx;
}

.fulimall-special-content{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.fulimall-special-item{
  width: 357rpx;
  height: 504rpx;
  margin: 0 12rpx 12rpx 0;
  background-color: #fff;
  border-radius: 20rpx;
}

.fulimall-special-item:nth-child(2n+2){
  margin-right: 0;
}

.fulimall-special-item-img{
  width: 100%;
  height: 336rpx;
  border-radius: 20rpx;
}

.fulimall-special-item-img image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}

.fulimall-special-item-content{
  padding: 0 18rpx;
  box-sizing: border-box;
}

.fulimall-special-item-name{
  line-height: 33rpx;
  color: #666;
  font-size: 22rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-top: 17rpx;
}

.fulimall-special-item-price{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.fulimall-special-item-left{
  color: #fe6601;
  font-size: 30rpx;
}

.fulimall-special-item-right{
  width: 100rpx;
  height: 35rpx;
  background-color: red;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  line-height: 35rpx;
}

.casualSnacks-nav{
  width: 100%;
  height: 49rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  display: flex;
  align-items: center;
}

.casualSnacks-nav-item{
  line-height: 45rpx;
  color: #333;
  font-size: 24rpx;
  margin-right: 52rpx;
  border-bottom: 2rpx solid #fff;
  display: inline-block;
}

.casualSnacks-nav-item:last-child{
  margin-right: 0;
}

.cactive{
  border-color: #fe6601;
  color: #fe6601;
}