.fulimall{
  width: 100%;
  background-color: #f5f5f5;
  padding: 12rpx;
  box-sizing: border-box;
}

.fulimall-title-img{
  width: 100%;
  height: 250rpx;
}

.fulimall-title-img image{
  width: 100%;
  height: 100%;
}

.fulimall-goods-wrapper{
  width: 100%;
  margin-top: 12rpx;
  display: flex;
  flex-wrap: wrap;
}

.fulimall-goods-item{
  width: 234rpx;
  height: 352rpx;
  background-color: #fff;
  margin: 0 12rpx 12rpx 0;
  border-radius: 10rpx;
}

.fulimall-goods-item:nth-child(3n+3){
  margin-right: 0;
}

.fulimall-goods-item-img{
  width: 100%;
  height: 232rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.fulimall-goods-item-img image{
  width: 100%;
  height: 100%;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}

.fulimall-goods-item-content{
  width: 100%;
  padding: 0 12rpx;
  box-sizing: border-box;
}

.fulimall-goods-item-name{
  color: #333;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.fulimall-goods-item-shop-price{
  color: #999;
  font-size: 20rpx;
  margin: 5rpx 0;
}

.fulimall-goods-item-price{
  color: #fe6601;
  font-size: 30rpx;
}

.import-goods-wrapper{
  width: 100%;
}

.import-goods-item{
  width: 100%;
  height: 216rpx;
  background-color: #fff;
  margin-top: 12rpx;
  border-radius: 5rpx;
  padding: 18rpx;
  box-sizing: border-box;
  display: flex;
}

.import-goods-item-img{
  width: 180rpx;
  height: 180rpx;
  border-radius: 5rpx;
}

.import-goods-item-img image{
  width: 100%;
  height: 100%;
  border-radius: 5rpx;
}

.import-goods-item-right{
  width: calc(100% - 180rpx);
  padding-left: 15rpx;
  position: relative;
}

.import-goods-item-title{
  color: #333;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.import-goods-item-subtitle{
  height: 62rpx;
  color: #666;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.import-goods-item-operation{
  position: absolute;
  bottom: 0;
  width: 100%;
  padding-right: 8rpx;
  box-sizing: border-box;
}

.present-price{
  color: #fe6601;
  font-size: 26rpx;
  margin-right: 5rpx;
}

.original-price{
  color: #999;
  font-size: 20rpx;
  margin-top: 5rpx;
}

.import-goods-item-price-btn{
  width: 120rpx;
  height: 44rpx;
  background-color: #fe6601;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 44rpx;
  border-radius: 5rpx;
}