.list-wrapper{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.list-head{
  width: 100%;
  height: 90rpx;
  border-bottom: 1rpx solid #ddd;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.list-head-input{
  width: 573rpx;
  height: 60rpx;
  background-color: #fff;
  border-radius: 30rpx;
  padding-right: 24rpx;
  box-sizing: border-box;
}

.list-head-input input{
  width: calc(100% - 34rpx);
  height: 60rpx;
  color: #999;
  font-size: 22rpx;
  padding-left: 26rpx;
}

.list-head-input image{
  width: 34rpx;
  height: 35rpx;
}

.list-operation{
  width: 100%;
  height: 77rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.list-operation-item{
  color: #000;
  font-size: 26rpx;
}

.listCur{
  color: #fe6601;
}